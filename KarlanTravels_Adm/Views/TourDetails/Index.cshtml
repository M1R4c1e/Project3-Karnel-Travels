        <th>
            @Html.DisplayNameFor(model => model.ActivityTimeStart)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ActivityTimeEnd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ActivityNote)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DeleteFlag)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
